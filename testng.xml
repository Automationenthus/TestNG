<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="ds-algo" parallel="tests" thread-count="2">
<listeners>
<listener class-name="listeners.ExtentwithListeners"></listener>
<listener class-name="listeners.AnnotationTransformerListener"></listener>
</listeners>
  <test name="dsAlgoModulesChrome">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
       
      <class name="tests.DataStructureIntroTest"/>
      <class name="tests.TreeTest"/>
      <class name="tests.RegisterTest"/>
      <class name="tests.ArrayTest"/>
      <class name="tests.GraphTest"/>
      <class name="tests.HomeTest"/>
      <class name="tests.LinkedListTest"/>
      <class name="tests.LoginTest"/>
      <class name="tests.StackTest"/>
      <class name="tests.QueueTest"/>
      
      
    </classes>
  </test>
     <test name="dsAlgoModulesFireFox">
  <parameter name="browser" value="firefox"></parameter>
  
      <classes>
       
     <class name="tests.DataStructureIntroTest"/>
      <class name="tests.TreeTest"/>
      <class name="tests.RegisterTest"/>
      <class name="tests.ArrayTest"/>
      <class name="tests.GraphTest"/>
      <class name="tests.HomeTest"/>
      <class name="tests.LinkedListTest"/>
      <class name="tests.LoginTest"/>
      <class name="tests.StackTest"/>
      <class name="tests.QueueTest"/>
      
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
